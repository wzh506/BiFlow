{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Single Train AFHQ256",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/train.py",
            "args": [
                "--dataset=afhq",
                "--img_size=256",
                "--channel_size=3",
                "--patch_size=8",
                "--channels=768",
                "--blocks=8",
                "--layers_per_block=8",
                "--noise_std=0.07",
                "--batch_size=4",
                "--epochs=4000",
                "--lr=1e-4",
                "--nvp",
                "--cfg=0",
                "--drop_label=0.1",
                "--sample_freq=200",
                "--logdir=runs/afhq256"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "prepare AFHQ256",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/prepare_fid_stats.py",
            "args": [
                "--dataset=afhq",
                "--img_size=256",
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
    ]
}